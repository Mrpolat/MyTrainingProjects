t = link.attr('sharetext').trim(); if (shareText.length > maxShareTextLength) { shareText = shareText.substring(0, maxShareTextLength); } remainingCharachters -= shareText.length + 1; tags = ' ' + shareText; } var title = document.title.trim(); if (title.length > remainingCharachters) { title = title.substring(0, remainingCharachters - 3) + "..."; } var tweet = title + tags; var url = 'https://twitter.com/share?text=' + encodeURIComponent(tweet) + '&url=' + encodeURIComponent(window.location.href); window.open(url); } function shareViaLinkedin(link) { var post = ""; if (!nullOrWhiteSpace(link.attr('additionaltext'))) { post += '&title=' + encodeURIComponent(link.attr('additionaltext')); } if (!nullOrWhiteSpace(link.attr('sharetext'))) { post += '&summary=' + encodeURIComponent(link.attr('sharetext')); } post += '&source=' + encodeURIComponent(document.title) + '&url=' + encodeURIComponent(window.location.href); var url = 'https://www.linkedin.com/shareArticle?mini=true' + post; window.open(url); } function shareViaGooglePlus() { var url = 'https://plus.google.com/share?url=' + encodeURIComponent(window.location.href); window.open(url); } function shareViaEmail(link) { var subject = nullOrWhiteSpace(link.attr('additionaltext')) ? document.title : link.attr('additionaltext'); var body = nullOrWhiteSpace(link.attr('sharetext')) ? window.location.href : link.attr('sharetext') + ' ' + window.location.href; var url = 'mailto:?subject=' + encodeURIComponent(subject) + '&body=' + encodeURIComponent(body); window.open(url, "_self"); } function printPage() { if (window.print) { window.print(); } } function nullOrWhiteSpace(string) { if (string != null && string.trim().length > 0) { return false; } return true; } </script><div id="support-container"><input name="support-bar-toggle" id="support-bar-toggle" type="checkbox" /><div id="support-bar"><label for="support-bar-toggle"><span>SUPPORT</span></label><div id="support-bar-icons" class="support-bar-items-container"><ul class=